# Environment Variables Template
# Copy this file to .env.development or .env.production and fill in your values

# Main backend API URL (NestJS - port 3003)
NEXT_PUBLIC_BASEURL=http://localhost:3003

# Dashboard & SMS API URL (Express - port 3004)
NEXT_PUBLIC_DASHBOARD_URL=http://localhost:3004

# Default locale and direction
NEXT_PUBLIC_DEFAULTLOCALE=fa
NEXT_PUBLIC_DEFAULTDIR=rtl
